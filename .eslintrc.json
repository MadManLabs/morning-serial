{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": "airbnb",
    "rules": {
        "no-extraneous-dependencies": 0,
        "global-require": 0,
        "react/jsx-filename-extension": 0,
        "arrow-parens": 0,
        "no-use-before-define": 0
    }
}
